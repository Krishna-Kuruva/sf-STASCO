        <modifyAllRecords>true</modifyAllRecords>
        <object>E_Rate_All_Request__c</object>
        <viewAllRecords>true</viewAllRecords>
    </objectPermissions>
    <userPermissions>
        <enabled>true</enabled>
        <name>ImportCustomObjects</name>
    </userPermissions>
</PermissionSet>
