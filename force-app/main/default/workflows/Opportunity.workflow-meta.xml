    <rules>
        <fullName>B2B Check Master Agreement</fullName>
        <active>true</active>
        <formula>AND(
OR($RecordType.Name = &quot;Risk Marketing Physical&quot;, $RecordType.Name = &quot;US - B2B Risk Marketing&quot;),

ISPICKVAL(StageName, &quot;Deal Done&quot;),

and(NOT(ISPICKVAL(Account.Master_Agreement__c, &quot;Not applicable&quot;)), NOT(ISPICKVAL(Account.Master_Agreement__c, &quot;Signed and attached&quot;))))</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>B2B Contract Not Checked Over 24 Hours</fullName>
        <active>true</active>
        <formula>AND( 
OR($RecordType.Name = &quot;UK - B2B Risk Marketing&quot;, $RecordType.Name = &quot;US - B2B Risk Marketing&quot;), 

ISPICKVAL(StageName, &quot;Deal Done&quot;),  


ISNULL(DEX_Reference__c) , 

Execution_Date__c  &lt; TODAY()-1)</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>B2B Jurisdictional Review Notification</fullName>
        <actions>
            <name>B2B_Jurisdiction_Review_Notification</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Opportunity.Review_Notification__c</field>
            <operation>equals</operation>
            <value>60</value>
        </criteriaItems>
        <description>Jurisdiction Review Notification</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>B2B PFE not entered within 24hours of deal being done</fullName>
        <active>true</active>
        <description>PFE needs to be entered with in 24 hours of the deal done</description>
        <formula>AND( 
OR($RecordType.Name = &quot;Risk Marketing Physical&quot;, $RecordType.Name = &quot;US - B2B Risk Marketing&quot;), 

ISPICKVAL(StageName, &quot;Deal Done&quot;),  


ISNULL( PFE_USD__c ) , 

Execution_Date__c  &lt; TODAY()-1)</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>B2B PFE not entered within 5 days of deal being done</fullName>
        <active>true</active>
        <formula>AND( 
OR($RecordType.Name = &quot;Risk Marketing Physical&quot;, $RecordType.Name = &quot;US - B2B Risk Marketing&quot;), 

ISPICKVAL(StageName, &quot;Deal Done&quot;),  


ISNULL( PFE_USD__c ) , 

Execution_Date__c  &lt; TODAY()-5)</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
