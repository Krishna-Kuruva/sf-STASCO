<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>RE_MY_Account_Number</fullName>
    <active>true</active>
    <description>Account number always numeric for Malaysia customer opps</description>
    <errorConditionFormula>AND ( RecordType.Name = &apos;Revolution MY&apos; ,NOT (ISNUMBER( AccountNumber )))</errorConditionFormula>
    <errorDisplayField>AccountNumber</errorDisplayField>
    <errorMessage>Account number always numeric</errorMessage>
</ValidationRule>
