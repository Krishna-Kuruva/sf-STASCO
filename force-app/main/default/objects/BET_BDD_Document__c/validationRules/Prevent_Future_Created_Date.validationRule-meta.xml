<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Prevent_Future_Created_Date</fullName>
    <active>true</active>
    <description>Prevents future date input for document created Date.</description>
    <errorConditionFormula>Created_Date__c &gt; TODAY()</errorConditionFormula>
    <errorDisplayField>BET_BDD_Form__c</errorDisplayField>
    <errorMessage>Cannot Enter a future date.</errorMessage>
</ValidationRule>
